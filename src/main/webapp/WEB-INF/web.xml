<?xml version="1.0" encoding="UTF-8"?>
<web-app version="6.0" xmlns="https://jakarta.ee/xml/ns/jakartaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="https://jakarta.ee/xml/ns/jakartaee https://jakarta.ee/xml/ns/jakartaee/web-app_6_0.xsd">
    <servlet>
        <servlet-name>LoginServlet</servlet-name>
        <servlet-class>controlador.LoginServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>LogoutServlet</servlet-name>
        <servlet-class>controlador.LogoutServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>AltaHabitacionServlet</servlet-name>
        <servlet-class>controlador.AltaHabitacionServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>SolicitarAlquilerServlet</servlet-name>
        <servlet-class>controlador.SolicitarAlquilerServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ControlarSolicitudServlet</servlet-name>
        <servlet-class>controlador.ControlarSolicitudServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>GestionarSolicitudServlet</servlet-name>
        <servlet-class>controlador.GestionarSolicitudServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>AltaPuntuacionServlet</servlet-name>
        <servlet-class>controlador.AltaPuntuacionServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>LoginServlet</servlet-name>
        <url-pattern>/LoginServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>LogoutServlet</servlet-name>
        <url-pattern>/LogoutServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>AltaHabitacionServlet</servlet-name>
        <url-pattern>/AltaHabitacionServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>SolicitarAlquilerServlet</servlet-name>
        <url-pattern>/SolicitarAlquilerServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ControlarSolicitudServlet</servlet-name>
        <url-pattern>/ControlarSolicitudServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>GestionarSolicitudServlet</servlet-name>
        <url-pattern>/GestionarSolicitudServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>AltaPuntuacionServlet</servlet-name>
        <url-pattern>/AltaPuntuacionServlet</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
    <welcome-file-list>
        <welcome-file>index.jsp</welcome-file>
    </welcome-file-list>
</web-app>
